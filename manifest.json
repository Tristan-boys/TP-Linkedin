{
    "manifest_version": 3,
    "name": "TPLinkedin",
    "version": "1.0.0",
    "description": "Prendre",
    "icons": {
      "128": "icons/icon-128.png"
    },
      "action": {
      "default_popup": "popup.html",
      "default_title": "TPLinkedin"
    },
    "background": {
      "service_worker": "background.js"
    },
    "permissions": ["storage", "activeTab", "tabs"],
    "host_permissions": ["https://api.linkedin.com/*"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "run_at": "document_idle"
      }
    ]
}